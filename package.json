{
  "name": "get-tsconfig",
  "version": "1.0.1",
  "description": "Utilities for finding and parsing TypeScript configuration files.",
  "main": "./src/index",
  "scripts": {
    "api-check": "api-extractor run",
    "build": "pack build",
    "test": "jest",
    "docs": "typedoc --theme markdown --out ./api-docs ./src"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/typeslick/get-tsconfig.git"
  },
  "author": {
    "name": "Jay Rylan",
    "email": "jay@jayrylan.com",
    "url": "https://jayrylan.com"
  },
  "license": "MIT",
  "bugs": "https://github.com/typeslick/get-tsconfig/issues",
  "homepage": "https://github.com/typeslick/get-tsconfig",
  "keywords": [
    "typescript",
    "tsconfig"
  ],
  "peerDependencies": {
    "typescript": "*"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.0",
    "@babel/core": "^7.2.2",
    "@babel/runtime-corejs2": "^7.2.0",
    "@loomble/cspell-dictionary": "*",
    "@microsoft/api-extractor": "^7.0.17",
    "@pika/pack": "^0.3.2",
    "@pika/plugin-build-node": "^0.3.11",
    "@pika/plugin-build-types": "^0.3.11",
    "@pika/plugin-build-web": "^0.3.11",
    "@pika/plugin-standard-pkg": "^0.3.11",
    "@types/babel__core": "^7.0.4",
    "@types/exit-hook": "^1.1.0",
    "@types/fs-extra": "^5.0.4",
    "@types/jest": "^24.0.6",
    "@types/jest-diff": "^20.0.1",
    "@types/node": "^11.9.4",
    "@types/write-file-atomic": "^2.1.1",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^24.1.0",
    "babel-preset-slick": "^7.0.4",
    "core-js": "^2.6.0",
    "cspell": "^3.1.3",
    "jest": "^24.1.0",
    "jest-serializer-path": "^0.1.15",
    "prettier": "^1.15.3",
    "semantic-release": "^15.13.0",
    "tsconfig-slick": "^3.0.2",
    "tslint": "^5.11.0",
    "tslint-slick": "^5.0.0",
    "typedoc": "^0.14.2",
    "typedoc-plugin-markdown": "^1.1.26",
    "typescript": "^3.3.3",
    "typescript-tslint-plugin": "^0.3.1",
    "weak": "^1.0.1"
  },
  "@pika/pack": {
    "pipeline": [
      [
        "@pika/plugin-standard-pkg",
        {
          "exclude": [
            "**/__*__/**/*",
            "**/website/**/*"
          ]
        }
      ],
      [
        "@pika/plugin-build-node"
      ],
      [
        "@pika/plugin-build-web"
      ],
      [
        "@pika/plugin-build-types"
      ]
    ]
  }
}
