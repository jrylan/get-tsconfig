{
  "$schema": "https://developer.microsoft.com/json-schemas/api-extractor/api-extractor.schema.json",
  "compiler": {
    "configType": "tsconfig",
    "rootFolder": "."
  },
  "project": {
    "entryPointSourceFile": "pkg/dist-types/index.d.ts"
  },
  "apiJsonFile": {
    "enabled": true,
    "outputFolder": "__meta__"
  },
  "apiReviewFile": {
    "apiReviewFolder": "__meta__/api-accepted",
    "enabled": true,
    "tempFolder": "__meta__/api-temp"
  }
}
